@{
    Layout = "";
}
@model ConfigurationModel
@using Nop.Plugin.Payments.Finans.Models;
@using Nop.Web.Framework;
@Html.Action("StoreScopeConfiguration", "Setting", new { area = "Admin" })
@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()
<table class="adminContent">
    <tr>
        <td class="adminTitle">
            @Html.OverrideStoreCheckboxFor(model => model.ApiFinans_OverrideForStore, model => model.ApiFinans, Model.ActiveStoreScopeConfiguration)
            @Html.NopLabelFor(model => model.ApiFinans):
        </td>
        <td class="adminData">
            @Html.EditorFor(model => model.ApiFinans)
            @Html.ValidationMessageFor(model => model.ApiFinans)
        </td>
    </tr>
    <tr>
        <td class="adminTitle">
            @Html.OverrideStoreCheckboxFor(model => model.MbrId_OverrideForStore, model => model.MbrId, Model.ActiveStoreScopeConfiguration)
