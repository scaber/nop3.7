    </tr>
    <tr>
        <td class="adminTitle">
            @Html.OverrideStoreCheckboxFor(model => model.FailUrl_OverrideForStore, model => model.FailUrl, Model.ActiveStoreScopeConfiguration)
            @Html.NopLabelFor(model => model.FailUrl):
        </td>
        <td class="adminData">
            @Html.EditorFor(model => model.FailUrl)
            @Html.ValidationMessageFor(model => model.FailUrl)
        </td>
    </tr> 
    <tr>
        <td colspan="2">
            <input type="submit" name="save" class="k-button" value="@T("Admin.Common.Save")" />
        </td>
    </tr>
</table>

}